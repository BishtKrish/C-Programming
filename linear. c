#include<stdio.h>
main() 
{
    int ar[10]={2,5,86,63,87,12,59,36,7,4};
    int i, num, count=0;
    printf("enter number you want search");
    scanf("%d",&num);
    for(i=0;i<10; i++)
    if(num==ar[i]) 
    {
        printf("%d is found at position %d",num,i);
        count++;
        break;
    }
    {
        if(count==0)
        {
            printf("%d is not given in array",num);
        }
    }
    return 0;
}
            
